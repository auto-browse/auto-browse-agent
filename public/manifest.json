{
    "name": "Auto Browse Agent",
    "version": "0.0.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "debugger",
        "background"
    ]
}