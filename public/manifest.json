{
    "manifest_version": 3,
    "name": "Auto Browse Agent",
    "description": "Auto-Browse Agent for automated browsing",
    "version": "0.0.2",
    "author": "Gurvinder Dhillon",
    "permissions": [
        "activeTab",
        "debugger"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    }
}